# API Keys Configuration Template
# 사용 방법: 이 파일을 복사해서 api_keys.yaml로 만들고 실제 키를 입력하세요
# Usage: Copy this file to api_keys.yaml and fill in your actual API keys

# OpenAI
openai:
  api_key: "your-openai-api-key-here"  # sk-...
  model: "gpt-4o-mini"

# Anthropic Claude
anthropic:
  api_key: "your-anthropic-api-key-here"  # sk-ant-...
  model: "claude-3-sonnet-20240229"

# Google Gemini
google:
  api_key: "your-google-api-key-here"  # AI...
  model: "gemini-1.5-flash"

# Upstage Solar
upstage:
  api_key: "your-upstage-api-key-here"  # up_...
  base_url: "https://api.upstage.ai/v1"
  model: "solar-10.7b"

# Local LLM (Day 4 연동)
local:
  endpoint: "http://localhost:8080"
  model: "local-llm"